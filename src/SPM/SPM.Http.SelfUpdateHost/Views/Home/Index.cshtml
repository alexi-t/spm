@{
    Layout = null;
}
@model List<SPM.Http.SelfUpdateHost.VersionRow>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
 @foreach (var version in Model)
 {
     <div>
         <h2>SPM@@@version.Version</h2>
         <h3>
             Created at @version.CreateTime 
             <a href="/api/update/download">Download</a>
         </h3>
     </div>
 }
</body>
</html>
